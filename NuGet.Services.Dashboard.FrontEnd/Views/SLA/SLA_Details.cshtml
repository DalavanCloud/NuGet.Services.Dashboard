<script src="~/Scripts/pagecontrol.js" type="text/javascript"></script>
<div class="page-control" data-role="page-control" style="height: 100%">
    <!-- Responsive controls -->
    <span class="menu-pull"></span>
    <div class="menu-pull-bar"></div>
    <!-- Tabs -->
    <ul>
        <li class="active"><a href="#1Hour">One Hour</a></li>
        <li><a href="#6Hours">Six Hours</a></li>
        <li><a href="#1Day">One Day</a></li>
        
    </ul>
    <!-- Tabs content -->
    <div class="frames">
        <div class="frame active" id="1Hour">@Html.Action("RequestsInOneHour", "SLA");</div>              
        <div class="frame" id="6Hours">@Html.Action("RequestsInSixHour", "SLA");</div>
        <div class="frame" id="1Day">@Html.Action("RequestsInOneDay", "SLA");</div>       
    </div>
</div>

