<script src="~/Scripts/pagecontrol.js" type="text/javascript"></script>
<div class="page-control" data-role="page-control" style="height: 100%">
    <!-- Responsive controls -->
    <span class="menu-pull"></span>
    <div class="menu-pull-bar"></div>
    <!-- Tabs -->
    <ul>
        <li class="active"><a href="#Requests">Requests Weekly Trend</a></li>
        <li><a href="#Errors">Errors Weekly Trend</a></li>
        <li><a href="#DataBase">Database Weekly Trend</a></li>
        
        
    </ul>
    <!-- Tabs content -->
    <div class="frames">
        <div class="frame active" id="Requests">@Html.Action("RequestsThisWeek", "SLA");</div>              
        <div class="frame" id="Errors">@Html.Action("ErrorsThisWeek", "SLA");</div>
        <div class="frame" id="DataBase">
            
           
                <div class="contentcpu">
                    @Html.Action("DBRequestsThisWeek", "ResourceMonitoring")
                </div>
           
          
                <div class="contenconnections">
                    @Html.Action("DBConnectionsThisWeek", "ResourceMonitoring")
                </div>
            </div>
        </div>
           
    </div>


