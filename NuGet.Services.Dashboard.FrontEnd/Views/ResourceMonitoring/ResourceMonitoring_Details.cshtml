<script src="~/Scripts/pagecontrol.js" type="text/javascript"></script>
<div class="page-control" data-role="page-control" style="height: 100%">
    <!-- Responsive controls -->
    <span class="menu-pull"></span>
    <div class="menu-pull-bar"></div>
    <!-- Tabs -->
    <ul>
        <li class="active"><a href="#CPU">CPU Usage</a></li>
        <li><a href="#Memory">Memory</a></li>
    </ul>
    <!-- Tabs content -->
    <div class="frames">
        <div class="frame active" id="CPU">
            <div class="grid grid-pad">
                <div class="col-1-3">
                    <div class="content">
                        @Html.Action("Instance0CPU") @Html.Action("Instance0CPU")
                       
                    </div>
                </div>
                <div class="col-1-3">
                    <div class="content">
                        @Html.Action("Instance1CPU") @Html.Action("Instance1CPU")
                    </div>
                </div>
                <div class="col-1-3">
                    <div class="content">
                        @Html.Action("Instance1CPU") @Html.Action("Instance2CPU")
                    </div>
                </div>
            </div>
            </div>
            <div class="frame" id="Memory"><div class="grid grid-pad">
    <div class="col-1-3">
        <div class="content">
       @Html.Action("Instance0Memory") @Html.Action("Instance0Memory")
        </div>
    </div>
    <div class="col-1-3">
        <div class="content">            
            @Html.Action("Instance0Memory") @Html.Action("Instance1Memory")
        </div>
    </div>
    <div class="col-1-3">
        <div class="content">
            @Html.Action("Instance0Memory") @Html.Action("Instance2Memory")
        </div>
    </div>
</div></div>           
        
