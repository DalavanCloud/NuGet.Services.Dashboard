@{
    ViewBag.Title = "SearchService_Detail";
}

<script src="~/Scripts/pagecontrol.js" type="text/javascript"></script>
<div class="page secondary with-sidebar">
    <div class="page-header">
        <div class="page-header-content" id="headercontent">
            <h1 id="title">SearchService<small>Detail</small></h1>
            <a href="/" id="backbutton" class=" back-button big page-back"></a>
        </div>
    </div>
</div>
<div class="page-control" data-role="page-control" style="height: 100%">
    <span class="menu-pull"></span>
    <div class="menu-pull-bar"></div>
    <ul>

        <li class="active"><a href="#CpuUsageToday">CPU usage today</a></li>
        <li><a href="#MemUsageToday">Memory usage today</a></li>
    </ul>
    <div class="frames">
        <div class="frame active" id="CpuUsageToday">@Html.Action("CpuUsage", "SearchService");</div>
        <div class="frame" id="MemUsageToday">@Html.Action("MemUsage", "SearchService");</div>
    </div>

</div>
